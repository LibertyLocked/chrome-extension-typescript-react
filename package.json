{
  "name": "chrome-extension-typescript-react",
  "version": "1.0.0",
  "description": "Chrome Extension starter kit, for React and TypeScript",
  "keywords": [
    "typescript",
    "react",
    "chrome-extension"
  ],
  "bugs": {
    "url": "https://github.com/LibertyLocked/chrome-extension-typescript-react/issues"
  },
  "author": "libertylocked",
  "contributors": [
    "chibat",
    "libertylocked"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/libertylocked/chrome-extension-typescript-react.git"
  },
  "scripts": {
    "start": "npm run-script watch",
    "build": "npm run-script build-prod",
    "watch": "cross-env NODE_ENV=development ./node_modules/.bin/webpack -w",
    "build-dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack",
    "build-prod": "cross-env NODE_ENV=production ./node_modules/.bin/webpack",
    "clean": "./node_modules/.bin/rimraf ./dist",
    "lint": "./node_modules/.bin/tslint -p tsconfig.json"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.47",
    "@types/jquery": "^3.2.6",
    "@types/node": "^8.0.9",
    "@types/react": "^15.0.35",
    "@types/react-dom": "^15.5.1",
    "@types/webpack": "^3.0.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.2.2",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "webpack": "^3.1.0"
  }
}
